{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "MS1 Generate SLDs",
  "type": "object",
  "additionalProperties": false,
  "required": ["slds"],
  "properties": {
    "slds": {
      "type": "array",
      "minItems": 1,
      "maxItems": 5000,
      "uniqueItems": true,
      "items": {
        "type": "string",
        "pattern": "^(?!-)[a-z0-9-]{2,63}(?<!-)$"
      }
    },
    "notes": {
      "type": "string",
      "maxLength": 2000
    }
  }
}
